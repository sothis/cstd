<?xml version="1.0" encoding="UTF-8"?>
<ufwb version="1.9">
    <grammar name="kfile" start="id:2" author="Janos Laube" email="janos.dev@gmail.com">
        <structure name="kfile_t" id="2" encoding="UTF-8" endian="little" signed="no">
            <structure name="kfile_preamble_t" id="3">
                <string name="magic" mustmatch="yes" id="4" fillcolor="FFFF00" type="zero-terminated">
                    <fixedvalues>
                        <fixedvalue name="magic" value="KFILE"/>
                    </fixedvalues>
                </string>
                <string name="version" id="5" fillcolor="00FF00" type="zero-terminated"/>
                <number name="cipher_data_bytes" id="6" fillcolor="5500FF" type="integer" length="8"/>
            </structure>
            <structure name="kfile_control_header_t" id="8">
                <number name="hash_function" id="9" type="integer" length="1"/>
                <number name="digest_bytes" id="10" type="integer" length="1"/>
                <number name="cipher_function" id="11" type="integer" length="1"/>
                <number name="cipher_mode" id="12" type="integer" length="1"/>
                <number name="key_bytes" id="13" type="integer" length="1"/>
                <number name="kdf_function" id="14" type="integer" length="1"/>
                <number name="kdf_complexity" id="15" type="integer" length="1"/>
            </structure>
            <structure name="kfile_kdf_header_t" id="17">
                <number name="kdf_salt_bytes" id="18" type="integer" length="1"/>
                <binary name="kdf_salt" id="19" length="kdf_salt_bytes+1"/>
            </structure>
            <structure name="kfile_iv_header_t" id="21">
                <number name="iv_bytes" id="22" type="integer" length="1"/>
                <binary name="iv" id="23" length="iv_bytes+1"/>
            </structure>
            <structure name="kfile_cipher_data_t" id="25">
                <binary name="cipher_data" id="26" length="kfile_preamble_t.cipher_data_bytes"/>
            </structure>
            <structure name="kfile_cipher_mac_t" id="28">
                <binary name="cipher_mac" id="29" length="kfile_control_header_t.digest_bytes+1"/>
            </structure>
        </structure>
    </grammar>
</ufwb>
